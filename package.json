{
  "name": "front-end",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-solid-svg-icons": "^5.8.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.2",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.3",
    "core-js": "^3.0.1",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1",
    "typescript": "^3.4.5",
    "react-app-env": "^1.2.3"
  },
  "scripts": {
    "build": "react-app-env --env-file=environments/${BUILD_ENV}.env build",
    "start": "react-app-env --env-file=environments/${BUILD_ENV}.env start",
    "build:dev": "BUILD_ENV=development npm run build",
    "build:prod": "BUILD_ENV=production npm run build",
    "start:dev": "BUILD_ENV=development npm run start",
    "start:prod": "BUILD_ENV=production npm run start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "heroku-postbuild": "npm run build:prod"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "react-redux": "^7.0.3",
    "redux": "^4.0.1"
  }
